public class Main {
    public static void main(String[] args) {
        int number = 4;
        int result = isEven(number);
        System.out.println(result);
    }

    public static int isEven(int number) {
        if (number % 2 == 0) {
            return 2;
        } else {
            return 1;
        }
    }
}

public class Main {
    public static void main(String[] args) {
        int number = 7;
        int result = isOdd(number);
        System.out.println(result);
    }

    public static int isOdd(int number) {
        if (number % 2 != 0) {
            return 2;
        } else {
            return 1;
        }
    }
}
